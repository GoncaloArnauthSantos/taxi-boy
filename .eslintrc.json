{
  "extends": ["next/core-web-vitals", "next/typescript"],
  "rules": {
    "semi": ["warn", "never"],
    "quotes": ["warn", "double"],
    "no-unused-vars": "warn",
    "no-console": "error",
    "indent": ["warn", 2],
    "comma-dangle": ["warn", "always-multiline"],
    "arrow-parens": ["warn", "always"]
  }
}
